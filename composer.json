{
    "require-dev": {
        "vimeo/psalm": "^4.23",
        "christophwurst/nextcloud": "v22.1.1",
        "nextcloud/coding-standard": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "scripts": {
        "cs:check": "php-cs-fixer fix --dry-run --diff",
        "cs:fix": "php-cs-fixer fix"
    }
}
